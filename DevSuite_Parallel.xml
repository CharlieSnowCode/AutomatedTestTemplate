<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="0" name="Dev_Suite_Parallel" parallel="methods" thread-count="3">

    <!-- use values "Dev" and "Test" -->
    <parameter name="Environment" value="Dev"/>
    <!-- use values "True" to run on server and "False" to run locally     -->
    <parameter name="Grid" value="False"/>
    <!-- use "true" to launch browser in incognito mode     -->
    <parameter name="Incognito" value="false"/>


    <test name="VisitWebSites">
        <classes>
            <class name="SeleniumTemplate"></class>
        </classes>
    </test>

</suite>
