<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="0" name="TestSuite_Sequential" >

    <!-- use values "Dev" and "Test" -->
    <parameter name="Environment" value="Test"/>
    <!-- use values "True" to run on the Selenium server and "False" to run locally     -->
    <parameter name="Grid" value="False"/>
    <!-- use "true" to launch browser in incognito mode     -->
    <parameter name="Incognito" value="true"/>


    <test name="VisitWebSites">
        <classes>
            <class name="TestTemplate"></class>
        </classes>
    </test>

</suite>
